<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.mytijian.mediator.common.DAO.SecuritySettingMapper">
	<resultMap id="settingResultMap" type="com.mytijian.mediator.common.DO.SecuritySettingDO" >
		<result column="id" property="id" jdbcType="INTEGER"/>
		<result column="gmt_created" property="gmtCreated" jdbcType="TIMESTAMP"/>
		<result column="gmt_modified" property="gmtModified" jdbcType="TIMESTAMP"/>
		<result column="hospital_id" property="hospitalId" jdbcType="INTEGER"/>
		<result column="version" property="version" jdbcType="INTEGER"/>
	</resultMap>
	
	<select id="selectAllSetting" resultMap="settingResultMap">
		select id,
		       gmt_created,
		       gmt_modified,
		       hospital_id,
		       version 
		FROM tb_mediator_security_setting 
	</select>
</mapper>